    "eslint-plugin-oxlint": "~1.1.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-inspect": "^11.3.2",
    "vite-plugin-markdown": "^2.2.0",
