    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "axios": "^1.11.0",
    "devicon": "^2.16.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue3-markdown-it": "^1.0.10"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tsconfig/node24": "^24.0.1",
